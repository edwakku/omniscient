<Window x:Class="omniscient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:omniscient"
        mc:Ignorable="d"
        Title="Omniscient 1.1" MouseDown="Window_MouseDown" Height="120.62" Width="600" Foreground="White" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterOwner" Visibility="Visible" AllowsTransparency="True" WindowStyle="None" Background="#D8151515">



    <Grid>
        <Label x:Name="CurrentlyOpen" Content="&gt; Current Window Title &lt;" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" Height="28" Width="580" FontFamily="Arial" FontSize="16" Background="#00000000" Foreground="White" RenderTransformOrigin="0,0"/>
        <Button x:Name="CloseButton" Click="closeClick" Content="x" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="41" Height="20" Background="{x:Null}" FontFamily="Arial" BorderBrush="#FF7289DA" Foreground="#FF7289DA"/>
        <Label Content="Omniscient 1.1" HorizontalAlignment="Center" Height="25" VerticalAlignment="Top" Width="162" FontFamily="Arial" Margin="211,8,227,0" Foreground="#FF7289DA"/>
