        <Rectangle Fill="#FF252525" HorizontalAlignment="Left" Width="600" Margin="0,98,0,0" Height="23" VerticalAlignment="Top"/>
        <Button x:Name="DiscordBtn" Content="Join the Discord" HorizontalAlignment="Left" VerticalAlignment="Top" Width="109" Margin="97,10,0,0" Height="20" Background="{x:Null}" Foreground="#FF7289DA" BorderBrush="#FF7289DA" FontFamily="Arial" FontSize="11" Click="Button_Click"/>
        <Label Content="bit.ly/omniscient-rpc - Created by: Sami (@morelikesame on Twitter)" HorizontalAlignment="Center" Height="23" VerticalAlignment="Top" Width="342" FontFamily="Arial" Margin="248,98,10,0" Foreground="#FF7289DA" FontSize="11"/>

    </Grid>
</Window>
